{
  "name": "visao-teologica",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run services:up && next dev",
    "services:up": "docker compose -f src/infra/compose.yaml up -d",
    "services:stop": "docker compose -f src/infra/compose.yaml stop",
    "services:down": "docker compose -f src/infra/compose.yaml down",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@phosphor-icons/react": "^2.0.14",
    "@prisma/client": "^5.5.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@types/node-fetch": "^2.6.8",
    "dayjs": "^1.11.10",
    "dayjs-plugin-utc": "^0.1.2",
    "googleapis": "^128.0.0",
    "next": "14.0.0",
    "next-auth": "^4.24.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.47.0",
    "resend": "^2.0.0",
    "swr": "^2.2.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@ts-safeql/eslint-plugin": "^2.0.3",
    "@types/jest": "^29.5.7",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-testing-library": "^6.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "libpg-query": "^15.0.2",
    "postcss": "^8",
    "prisma": "^5.5.2",
    "tailwindcss": "^3",
    "ts-jest": "^29.1.1",
    "typescript": "^5",
    "whatwg-fetch": "^3.6.19"
  }
