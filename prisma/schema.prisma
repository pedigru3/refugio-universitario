generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              String  @id @default(uuid())
  username        String  @unique
  name            String
  course          String?
  education_level String?

  created_at DateTime @default(now())

  @@map("users")
}
